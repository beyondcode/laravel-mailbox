{
    "name": "beyondcode/laravel-mailbox",
    "description": "Handle incoming emails in your Laravel application.",
    "keywords": [
        "beyondcode",
        "laravel-mailbox"
    ],
    "homepage": "https://github.com/beyondcode/laravel-mailbox",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de",
            "homepage": "https://beyondcode.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/container": "5.8.*",
        "illuminate/database": "^5.8",
        "illuminate/log": "5.8.*",
        "illuminate/routing": "5.8.*",
        "illuminate/support": "5.8.*",
        "willdurand/email-reply-parser": "^2.8",
        "zbateson/mail-mime-parser": "^1.1",
        "beyondcode/laravel-mailbox": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.5",
        "zendframework/zend-mail": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "BeyondCode\\Mailbox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeyondCode\\Mailbox\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BeyondCode\\Mailbox\\MailboxServiceProvider"
            ],
            "aliases": {
                "Mailbox": "BeyondCode\\Mailbox\\Facades\\Mailbox"
            }
        }
    }
}
